<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<connectionStrings>
		<!--
  IMPORTANT:
  Everyone must create and work on DrinkDB_Dev database.
  Replace Server=(localdb)\ProjectModels with:
    - Local SQL Server: localhost\SQLEXPRESS or your instance name
    - Docker SQL Server: localhost,1433 (or your exposed port)
-->

		<add name="DrinkDbConnection"
			 connectionString="Server=(localdb)\ProjectModels;Database=DrinkDB_Dev;Trusted_Connection=True;TrustServerCertificate=True;"
			 providerName="System.Data.SqlClient" />
	</connectionStrings>

	<appSettings>
		<!-- Replace these with your actual Google OAuth credentials -->
		<add key="GoogleClientId" value="YOUR_CLIENT_ID" />
		<add key="GoogleClientSecret" value="YOUR_CLIENT_SECRET" />
	</appSettings>

</configuration>